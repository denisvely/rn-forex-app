{
  "expo": {
    "name": "Exa-Dynamic",
    "slug": "Exa-Dynamic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.exadynamic",
      "buildNumber": "1",
      "usesIcloudStorage": true
    },
    "android": {
      "package": "com.exadynamic",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "useNextNotificationsApi": true
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "Exa-dynamic",
            "project": "Exa-dynamic",
            "authToken": "86ee57644cea4219910884aafbe5d0efe50409b2686940f5a3b5584f00ed2278"
          }
        }
      ]
    }
  }
}
