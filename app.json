{
  "expo": {
    "name": "Exa-Dynamic",
    "slug": "Exa-Dynamic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.exadynamic",
      "buildNumber": "1",
      "usesIcloudStorage": true
    },
    "android": {
      "package": "com.exadynamic",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "useNextNotificationsApi": true
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
